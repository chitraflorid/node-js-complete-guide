<%# User List Page EJS Template %>

<%- include('includes/head.ejs') %>

<h1>User List</h1>

<% if (users.length > 0) { %>

    <ul>
        <% users.forEach( (user) => { %>
            <li><%= user.name %></li>
        <% }); %>
    </ul>

<% } else { %>
    <h1>No Users Found!</h1>
<% } %>

<%- include('includes/end.ejs') %>
